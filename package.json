{
  "name": "@entropy-cycle/pentapoles",
  "version": "0.0.2",
  "description": "Simulate reality",
  "directories": {
    "test": "test"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.ts",
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf ./lib",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:types": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",

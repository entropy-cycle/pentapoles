    "@web/test-runner-mocha": "^0.7.5",
    "chai": "^4.3.7",
    "es-dev-server": "^2.1.0",
    "esbuild": "^0.15.12",
    "eslint": "^8.25.0",
    "mocha": "^10.2.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "typescript-eslint": "0.0.1-alpha.0"
  },
